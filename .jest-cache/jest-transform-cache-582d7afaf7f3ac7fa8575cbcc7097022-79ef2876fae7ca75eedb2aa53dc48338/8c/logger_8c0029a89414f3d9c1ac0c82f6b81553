"use strict";
Object.defineProperty(exports, "__esModule", {
    value: true
});
Object.defineProperty(exports, "logger", {
    enumerable: true,
    get: function() {
        return logger;
    }
});
const levelPriority = {
    debug: 10,
    info: 20,
    warn: 30,
    error: 40
};
function getEnvLogLevel() {
    const level = process.env.NEXT_PUBLIC_LOG_LEVEL;
    if (level && levelPriority[level]) return level;
    return 'info';
}
class Logger {
    constructor(){
        this.level = getEnvLogLevel();
    }
    setLevel(level) {
        this.level = level;
    }
    shouldLog(level) {
        return levelPriority[level] >= levelPriority[this.level];
    }
    debug(...args) {
        if (this.shouldLog('debug')) console.debug(...args);
    }
    info(...args) {
        if (this.shouldLog('info')) console.info(...args);
    }
    warn(...args) {
        if (this.shouldLog('warn')) console.warn(...args);
    }
    error(...args) {
        if (this.shouldLog('error')) console.error(...args);
    }
}
const logger = new Logger();

